{
    "name": "laravel/zibal",
    "description": "A simple Laravel package for zibal",
    "type": "laravel-package",
    "license": "JEY",
    "keywords": [
        "laravel",
        "zibal",
        "jey",
        "gateway"
    ],
    "authors": [
        {
            "name": "Javad Sadeghi",
            "email": "sadeghij87@gmail.com"
        }
    ],
    "require": {
        "zibal/php": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Zibal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zibal\\ServiceProvider"
            ],
            "aliases": {
                "Zibal": "Zibal\\Facade"
            }
        }
    }
}
